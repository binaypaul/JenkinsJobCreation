pipeline {
agent
    {
        node {
                label 'master'
                customWorkspace "${env.JobPath}"
              }
    }
    stages 
    {
        stage('Start') {
            steps {
                echo 'Hello.. Starting...'
            }
        }
        stage ('Invoke_MyFirstSecondJob') {
            steps {
                build job: 'MyFirstJob', parameters: [string(name: 'NAME', value: 'Animesh'), booleanParam(name: 'Gender', value: true)]
                build job: 'MySecondJob', parameters: [string(name: 'NAME', value: 'Anamika'), booleanParam(name: 'Gender', value: false)]
            }
        }
        stage('End') {
            steps {
                echo 'Bye.. Finished.'
            }
        }
    }
}
