pipeline {
agent
    {
        node {
                label 'master'
                customWorkspace "${env.JobPath}"
              }
    }
    stages 
    {
        stage('Start') {
            steps {
                echo 'Hello.. Starting...'
            }
        }
        stage ('Invoke_MyFirstSecondJob') {
            steps {
                build job: 'MyFirstJob', 
                parameters: [
                    string(name: 'NAME', value: 'Binay'), 
                    booleanParam(name: 'Gender', value: true)
                ]
            }
        }
        stage('End') {
            steps {
                echo 'Bye.. Finished.'
            }
        }
    }
}
